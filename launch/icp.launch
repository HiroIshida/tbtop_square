<launch>
  <node name="converter" pkg="tbtop_square" type="converter">
    <remap from="output" to="vase_pc"/>
  </node>

  <node name="icp_registration"
        pkg="nodelet" type="nodelet"
        args="standalone jsk_pcl/ICPRegistration"
        output="screen">
    <remap from="~input" to="kinect_head/depth_registered/points" />
    <remap from="~input_reference" to="vase_pc" />
    <remap from="~input/camera_info" to="kinect_head/depth_registered/camera_info" />
  </node>

</launch>


